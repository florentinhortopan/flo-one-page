{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    },
    {
      "src": "password-protection.js",
      "use": "@vercel/static"
    },
    {
      "src": "popups/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "popups/**/*.{mp4,mov,m4v,mp3,jpg,jpeg,png,gif,webp}",
      "use": "@vercel/static"
    },
    {
      "src": "popups/**/*.pdf",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/styles.css",
      "dest": "/styles.css"
    },
    {
      "src": "/script.js", 
      "dest": "/script.js"
    },
    {
      "src": "/password-protection.js",
      "dest": "/password-protection.js"
    },
    {
      "src": "/popups/(.*)",
      "dest": "/popups/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}