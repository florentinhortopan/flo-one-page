{
  "version": 2,
  "buildCommand": "git lfs pull",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "."
      }
    }
  ],
  "routes": [
    {
      "src": "/styles.css",
      "dest": "/styles.css"
    },
    {
      "src": "/script.js", 
      "dest": "/script.js"
    },
    {
      "src": "/password-protection.js",
      "dest": "/password-protection.js"
    },
    {
      "src": "/popups/(.*)",
      "dest": "/popups/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}